#
# Expires on "XYZ XX, 20XX" (90 days)
homelab_auth_key=MY-SUPER-DUPER-VALUE
# Expires on "XYZ XX, 20X" (90 days)
cloudybox_auth_key=MY-SUPER-DUPER-VALUE

[neko]
