# NOTE:
# 1. Rename file to "credentials.ini"
# 2. Fill below with appropriate values
# 3. or write script to fetch values dynamically off Secrets Manager
# 4. Profit!
[proxmox]
url=https://my-proxmox-server:8006
host=my-proxmox-server
user=root@pam
password=MY-SUPER-DUPER-VALUE
domain=MY-SUPER-DUPER-VALUE

[cloudflare]
email=MY-SUPER-DUPER-VALUE
api_token=MY-SUPER-DUPER-VALUE
domain=MY-SUPER-DUPER-VALUE

[cloudflare-ddns]
api_token=MY-SUPER-DUPER-VALUE
zone=MY-SUPER-DUPER-VALUE
subdomain=MY-SUPER-DUPER-VALUE

[tailscale]
